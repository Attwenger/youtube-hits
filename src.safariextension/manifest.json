{
  "name": "YouTube Hits",
  "short_name": "iyhits",
  "description": "YouTube Hits finds trending (hot) songs for you from reddit.com/r/Music",
  "author": "Richard Neomy",
  "version": "0.1.0",
  "manifest_version": 2,
  "permissions": [
    "storage",
    "tabs",
    "notifications",
    "https://www.reddit.com/r/*"
  ],
  "web_accessible_resources": [],
  "browser_action": {
    "default_icon": {
      "19": "data/icons/19.png",
      "38": "data/icons/38.png"
    }
  },
  "background": {
    "page": "lib/chrome/background.html"
  },
  "options_page": "data/options/index.html",
  "homepage_url": "http://add0n.com/index.html",
  "icons": {
    "16": "data/icons/16.png",
    "48": "data/icons/48.png",
    "128": "data/icons/128.png"
  }
}
